=======>    expand $
=======> 	remove " && '
=======> 	echo"jfldhsfkhekjl"jkhdkjgd


// pipe errors

=======>	||  -> syntax error near unexpected token `||'
=======>	||| -> syntax error near unexpected token `||'
=======>	|   -> syntax error near unexpected token `|'+
=======>	|/	-> syntax error near unexpected token `|'
=======>	|$  -> syntax error near unexpected token `|'
=======>	|\" -> syntax error near unexpected token `|'
=======>	|\' -> syntax error near unexpected token `|'
=======>	cmd  |   |  cmd ----> syntax error near unexpected token `|'


// redir_in error

=======>	<	-> syntax error near unexpected token `newline'
=======>	<       -> syntax error near unexpected token `newline'
=======>	<<	-> syntax error near unexpected token `newline'
=======>	<<      -> syntax error near unexpected token `newline'
=======>	<<<	-> syntax error near unexpected token `newline'
=======>	<<<<	-> syntax error near unexpected token `<'
=======>	<<<<<	-> syntax error near unexpected token `<<'
=======>	<<<<<<	-> syntax error near unexpected token `<<<'
=======>	<>	-> syntax error near unexpected token `newline'
=======>	<<>	-> syntax error near unexpected token `>'
=======>	<<>>	-> syntax error near unexpected token `>>'
=======>	<<>>>>	-> syntax error near unexpected token `>>'
=======>	>| ->	 syntax error near unexpected token `newline'
=======>	|>	->	syntax error near unexpected token `|'


// redir_out error

=======>	> 	->	syntax error near unexpected token `newline'
=======>	>      -> syntax error near unexpected token `newline'
=======>	>>	-> syntax error near unexpected token `newline'
=======>	>>       -> syntax error near unexpected token `newline'
=======>	>>|    -> syntax error near unexpected token `|'
=======>	>>||   -> syntax error near unexpected token `||'
=======>	>>|||   -> syntax error near unexpected token `||'
=======>	>>>		-> syntax error near unexpected token `>'
=======>	>>>>>>> -> syntax error near unexpected token `>>'
=======>	>><		-> syntax error near unexpected token `<'
=======>	>><<	-> syntax error near unexpected token `<<'
=======>	>><<<	->	syntax error near unexpected token `<<<'
=======>	>><<<<<  -> syntax error near unexpected token `<<<'


ls > out1 > out2
ls >> file > file2
ls < file < f
ls << f <s